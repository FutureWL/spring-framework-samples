<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorld" class="io.github.futurewl.spring.framework.beans.event.demo1.HelloWorld">
        <property name="message" value="Hello World!"/>
    </bean>

    <bean id="cStartEventHandler" class="io.github.futurewl.spring.framework.beans.event.demo1.CStartEventHandler"/>

    <bean id="cStopEventHandler" class="io.github.futurewl.spring.framework.beans.event.demo1.CStopEventHandler"/>

</beans>
